\set QUIET 1
\pset pager off
\timing
\pset format wrapped
\pset linestyle unicode
\pset border 2
\pset null '[NULL]'
\set HISTCONTROL ignoredups
\set COMP_KEYWORD_CASE upper
\set VERBOSITY verbose
\set PROMPT1 '%[%033[1m%]%x%n@%m:%>/%/%R%[%033[0m%]%# '
\set PROMPT2 '[more] %R > '
\set ON_ERROR_ROLLBACK interactive
\unset QUIET
